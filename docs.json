{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Mint Starter Kit",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "平台",
        "groups": [
          {
            "group": "平台介绍",
            "pages": [
              "introduction",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "系统功能",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/settings",
              "essentials/navigation",
              "essentials/reusable-snippets"
            ]
          }
        ]
      },
      {
        "tab": "产品",
        "groups": [
          {
            "group": "产品文档",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "更新日志",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "平台首页",
          "href": "https://www.jqbao168.com",
          "icon": "home"
        },
        {
          "anchor": "隐私申明",
          "href": "https://www.jqbao168.com/privacy-statement.html",
          "icon": "slack"
        },
        {
          "anchor": "联系我们",
          "href": "https://www.jqbao168.com/contact.html",
          "icon": "telephone"
        }
      ]
    }
  },
  "logo": {
    "light": "/images/hero-light.svg",
    "dark": "/images/hero-dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "转到平台首页",
        "href": "https://www.jqbao168.com"
      },
      {
        "label": "产品",
        "href": "https://github.com/mintlify-ui/mintlify-ui"
      }
    ],
    "primary": {
      "type": "button",
      "label": "登录",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  }
}